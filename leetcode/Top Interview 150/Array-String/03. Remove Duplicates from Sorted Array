#include <iostream>
#include <vector>
using namespace std;

class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int n = nums.size();
        if (n <= 1) return n;

        int* p = &nums[0];
        int* q = &nums[1];

        while(q != &nums[n]){
            if(*p != *q){
                p++;
                *p = *q;
            }
            q++;
        }

        return p - &nums[0] + 1;
    }
};

int main()
{
    Solution solution;
    vector<int> nums = {0, 0, 1, 1, 1, 2, 2, 3, 3, 4};

    int rst = solution.removeDuplicates(nums);
    cout << rst << endl;

    for(int i=0; i<rst; i++){
        cout << nums[i] << ' ';
    }
    return 0;
}
