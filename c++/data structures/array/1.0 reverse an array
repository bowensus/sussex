#include <iostream>
using namespace std;

void swap(int* p, int* q)
{
    int ret;
    ret = *p;
    *p = *q;
    *q = ret;
    p++;
    q--;
    if(q > p){
        swap(p, q);
    }
}

int main()
{
    int a[] = {1, 2, 3, 4, 5};
    int n = sizeof(a) / sizeof(a[0]);
    swap(&a[0], &a[n-1]);
    for(int i=0; i<n; i++){
        cout << a[i] << ' ';
    }

    return 0;
}
